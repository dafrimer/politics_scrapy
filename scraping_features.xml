<?xml version="1.0" encoding="utf-8" ?>
<scraperules>
    <news_outlet name="Huffington Post" domain="huffingtonpost">
        <url_focus>http://www.huffingtonpost.com/section/politics</url_focus>
        <MainPageManage>
            <splash></splash>
            <main_page_headlines_xpath>//div[@class="card__headlines"]/h2[@class = "card__headline js-card-headline"]/a</main_page_headlines_xpath>
            <historical_main_page_headlines_xpath>//a[re:@href</historical_main_page_headlines_xpath>
        </MainPageManage>
        <ArticleManage>
            <checkForPolitics>//span[@class="entry-eyebrow"]/span/a/text()</checkForPolitics>
            <publish_date>//header[@class="entry__header bn-entry-header"]/div[@class="timestamp"]/span[@class="timestamp__date--published"]/text()</publish_date>
            <headline_title>//h1[@class="headline__title"]/text()</headline_title>
            <body keepall="p">div[@class="entry__body js-entry-body"]</body>
            <optional_tags type="list">//div[@class="tag-cloud js-tag-cloud bn-tag-cloud"]/a</optional_tags>
            <author>//a[@class="author-card__details__link bn-author-name bn-clickable"]/text()</author>
        </ArticleManage>
    </news_outlet>

    <news_outlet name="Politico" domain="politico">
        <url_focus>http://www.politico.com/politics</url_focus>
        <MainPageManage>
            <main_page_headlines_xpath>[Insert links to look for on main page]</main_page_headlines_xpath>
        </MainPageManage>
        <ArticleManage>
            <headline_title>//div[@class="summary  "]/*/h1/text()</headline_title>
            <publish_date>//header[@class="entry__header bn-entry-header"]/div[@class="timestamp"]/span[@class="timestamp__date--published"]/text()</publish_date>

            <body keepall="p">//div[class="story-text"]</body>
            <optional_tags type="list">//div[@class="tag-cloud js-tag-cloud bn-tag-cloud"]/a</optional_tags>
            <author>///footer[@class="meta"]//span[@class="vcard"]/a[@rel="author"]/text()</author>
        </ArticleManage>
    </news_outlet>

